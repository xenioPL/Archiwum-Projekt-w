#include <iostream>
#include <cmath>
#include <cstdio>
using namespace std;


int main()
{
    double a;
    cin>>a;
    double x1,x2,x3;
    double y1,y2,y3;
   for(double i = 0;i<a;i++)
   {
    cin>>x1>>y1>>x2>>y2>>x3>>y3;
    int macierz = ((x1*y2) + (x2*y3) + (x3*y1)-(x3*y2)-(x2*y1)-(x1*y3) );
    if(macierz== 0)
        cout<<"TAK"<<endl;
        else
            cout<<"NIE"<<endl;
   }
    return 0;
}
